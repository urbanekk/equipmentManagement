<Window x:Class="equipmentMangement.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:equipmentMangement"
        mc:Ignorable="d"
        Title="LoginWindow" Height="250" Width="400">
    <StackPanel>
        <Label Margin="10">Login:</Label>
        <TextBox x:Name="login" Margin="10"></TextBox>
        <Label Margin="10">Hasło:</Label>
        <PasswordBox x:Name="passtxt" Margin="10"></PasswordBox>
        <Button Click="Button_Click" Margin="10">LogIn</Button>
    </StackPanel>
</Window>
